{
  "containerConfig": [
    {
      "id": "java-20",
      "image": "eclipse-temurin:20-jdk",
      "compilationCmd": "javac *.java",
      "executionCmd": "java Main",
      "reserveContainerAmount": 0
    },
    {
      "id": "junit-5",
      "image": "eclipse-temurin:20-jdk",
      "compilationCmd": "javac -d target -cp target:resources/junit-platform-console-standalone-1.9.3.jar *.java",
      "executionCmd": "java -jar resources/junit-platform-console-standalone-1.9.3.jar --class-path target --select-class PrinterTest --reports-dir=./reports --details=tree",
      "add": ["./resources/junit-platform-console-standalone-1.9.3.jar"],
      "reserveContainerAmount": 0
    },
    {
      "id": "script",
      "image": "alpine:latest",
      "executionCmd": "./test.sh",
      "reserveContainerAmount": 0
    }
  ]
}